{
  "collection_name": "customer_support_tickets",
  "description": "Multi-tenant customer support system with partitioning",
  "enable_dynamic_field": true,
  "fields": [
    {
      "name": "ticket_id",
      "type": "Int64",
      "is_primary": true,
      "auto_id": true,
      "description": "Auto-generated support ticket ID"
    },
    {
      "name": "company_id",
      "type": "VarChar",
      "max_length": 50,
      "is_partition_key": true,
      "description": "Company identifier for tenant isolation"
    },
    {
      "name": "issue_description",
      "type": "VarChar",
      "max_length": 2000,
      "description": "Customer issue description"
    },
    {
      "name": "priority_level",
      "type": "Int32",
      "min": 1,
      "max": 5,
      "description": "Issue priority (1=low, 5=critical)"
    },
    {
      "name": "issue_embedding",
      "type": "FloatVector",
      "dim": 256,
      "description": "Issue semantic embedding for similarity matching"
    }
  ]
}
